<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ayoub.xo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/xogame"
        android:label="@string/app_name"
        android:roundIcon="@drawable/xogame"
        android:supportsRtl="true"
        android:theme="@style/Theme.Xo">
        <activity android:name=".basicphrase"></activity>
        <activity android:name=".webvie" />
        <activity android:name=".mediaplayer" />
        <activity android:name=".videoplayer" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-1299787344129628~5054062422" />
    </application>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>